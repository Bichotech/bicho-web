---
import Layout from '../layouts/Layout.astro';
import MainSection from '../components/main-section.astro';
import Customers from '../components/customers.astro';
// import SimpleSection from '../components/simple-section.astro';
---

<Layout title="Gracias por confiar en BichoTech" desc="Gracias a nuestros clientes, BichoTech crece a pasos agigantados.">
	
	<MainSection
			title="Clientes"
			background="wall_07.webp"
			element={[ "gracias", "por tu", "confianza"]} />

	<Customers CustomersArray={[
		[
			'logo-de-la-clinica-dental-carranza.webp',
			'Dental Carranza del Dr. Ramiro Carranza',
			'/casos-de-estudio/dental-carranza'
		],
		[
			'logo-para-el-artista-plastico-heber-banda.webp',
			'Heber Banda, artísta plástico'
		],
		[
			'logo-de-la-empresa-mooving-logistics.webp',
			'Mooving Logistics, servicios de logística en Monterrey, NL'
		],
		[
			'logo-del-negocio-malenky.webp',
			'Malenky es nuestro primer cliente creando personajes en crochet'
		]
	]} />
</Layout>